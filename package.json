{
  "name": "root",
  "private": true,
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "prebuild--disabled": "run-p -c lint:*",
    "build": "lerna exec 'BABEL_ENV=build babel src --out-dir dist --source-maps --extensions .ts,.tsx --config-file ../../babel.config.js --delete-dir-on-start --no-comments'",
    "lint:css": "stylelint 'packages/**/*.ts{,x}'",
    "lint:ts": "tslint 'packages/**/*.ts{,x}'",
    "lint": "run-p -c lint:*",
    "test": "jest",
    "tsc": "tsc",
    "storybook": "yarn start-storybook -p 9009",
    "build-storybook": "build-storybook"
  },
  "workspaces": [
    "packages/*",
    "src/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.0-rc.1",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.3.0-rc.1",
    "@storybook/addons": "^5.3.0-rc.1",
    "@storybook/preset-create-react-app": "^1.5.0",
    "@storybook/react": "^5.3.9",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "dotenv": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.0.0-beta6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^24.9.0",
    "lerna": "^3.20.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-scripts": "^3.3.0",
    "rimraf": "^2.6.2",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "slate": "0.57.1",
    "slate-react": "https://gitpkg.now.sh/SvyatoslavPozhydaev/slate/packages/slate-react?master"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
